#include <bits/stdc++.h>
using namespace std;

int main() {
    int t;
    cin>>t;
    while(t--){
    int n;
    cin>>n;
    vector<int>leo(n);
    for (int i = 0; i < n; ++i)
    {
        cin>>leo[i];
    }
    int cnt2=0;
    for (int i = 0; i < n; ++i)
    {
        if(leo[i]==2){
            cnt2++;
        }
    }
    if(cnt2%8==0){
        cout<<"Yes"<<'\n';
    }
    else{
        cout<<"No"<<'\n';
    }
   } 
    
    return 0;
}

// using lambda function
#include <bits/stdc++.h>
using namespace std;

int main() {
    int t;
    cin>>t;
    while(t--){
    int n;
    cin>>n;
    vector<int>leo(n);
    for (int i = 0; i < n; ++i)
    {
        cin>>leo[i];
    }
    int cnt2=0;
    for (int i = 0; i < n; ++i)
    {
        if(leo[i]==2){
            cnt2++;
        }
    }
    if(cnt2%8==0){
        cout<<"Yes"<<'\n';
    }
    else{
        cout<<"No"<<'\n';
    }
   } 
    
    return 0;
}
